@using ETicaretMvc.Domain.Entities;
@model List<Product>
@{
	Layout=null;
}
<h1>hello</h1>

<table class="table table-bordered">
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>Price</th>
		<th>Stok</th>
	</tr>
	@foreach (var product in Model)
	{
		<tr>
			<td>@product.Id</td>
			<td>@product.Name</td>
			<td>@product.Price</td>
			<td>@product.Stock</td>
		</tr>
	}
</table>